<launch>
    <node pkg="ackermann_vehicle_gazebo" type="linang_to_ackermann.py" name="linang_to_ackermann" output="screen">
        <param name="vel_topic" value="/vel_cmd"/>
        <param name="angle_topic" value="/angle_cmd"/>
        <param name="ackermann_cmd_topic" value="/ackermann_vehicle/ackermann_cmd"/>
	<param name="wheelbase" value="0.335"/>
